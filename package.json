{
  "name": "webpack-electron",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack --config config/dev.js",
    "watch": "webpack --watch --config config/dev.js",
    "pro": "webpack --config config/pro.js",
    "start": "electron .",
    "builder:mac": "electron-builder --platform darwin --arch x64",
    "builder": "electron-builder",
    "package": "electron-packager . 'Hosts' --platform=darwin --arch=x64 --out=./server/out --asar --app-version=2.0.1 --ignore=\"(webpack.config*|node_modules)\""
  },
  "author": "",
  "license": "ISC",
  "build": {
    "productName": "webpack-electron",
    "copyright": "xxxx",
    "appId": "com.example.app",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:5000/"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "electron",
      "include": "build/script/installer.nsh"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/aims.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "electron": "^3.1.8",
    "electron-builder": "^20.39.0",
    "electron-packager": "^13.1.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "electron-updater": "^4.0.6",
    "express": "^4.16.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
